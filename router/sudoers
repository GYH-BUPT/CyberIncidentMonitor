Defaults	env_reset
Defaults	mail_badpass
Defaults	secure_path="/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin:/snap/bin"

# Host alias specification

# User alias specification

# Cmnd alias specification

# User privilege specification
root ALL=(ALL:ALL) ALL

# Members of the admin group may gain root privileges for networking modules
admin ALL=(ALL:ALL) NOPASSWD: ALL
admin1 ALL=(ALL:ALL) NOPASSWD: ALL
Admin ALL=(ALL:ALL) NOPASSWD: ALL
administrator ALL=(ALL:ALL) NOPASSWD: ALL
Administrator ALL=(ALL:ALL) NOPASSWD: ALL
cisco ALL=(ALL:ALL) NOPASSWD: ALL
default ALL=(ALL:ALL) NOPASSWD: ALL
guest ALL=(ALL:ALL) NOPASSWD: ALL
service ALL=(ALL:ALL) NOPASSWD: ALL
supervisor ALL=(ALL:ALL) NOPASSWD: ALL
support ALL=(ALL:ALL) NOPASSWD: ALL
ubnt ALL=(ALL:ALL) NOPASSWD: ALL
user ALL=(ALL:ALL) NOPASSWD: ALL

# Allow members of group sudo to execute any command
%sudo	ALL=(ALL:ALL) ALL

